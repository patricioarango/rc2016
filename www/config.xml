<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegaptips.brainvscrain23" version="1" versionCode = "1" xmlns="http://www.w3.org/ns/widgets"  xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Brain vs. Crane</name>
    <description>
        Para saber qué carreras habrá el fin de semana.
    </description>
    <author email="patricioarango@gmail.com" href="http://plus.google.com/+PatricioArango">
        @patricioarango
    </author>
    <icon src="icon.png" />
    <access origin="*" />
    <preference name='phonegap-version' value='cli-5.1.1' />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="orientation" value="portrait" />
      <gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
<gap:plugin name="phonegap-plugin-push" source="npm" />
<gap:plugin name="cordova-plugin-whitelist" source="npm" />
      <icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
     <preference name="permissions" value="none" />
     <allow-navigation href="http://autowikipedia.es/*" />
  
    

    <preference name="StatusBarBackgroundColor" value="#3498db" />
</widget>
